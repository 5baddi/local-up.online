{
    "name": "5baddi/local-up.online",
    "type": "project",
    "description": "Easy tools to shape your affiliate program.",
    "keywords": ["tool", "shopify", "affiliate"],
    "authors": [
        {
            "name": "BADDI Services",
            "email": "services@baddi.info",
            "homepage": "https://baddi.info"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "2checkout/2checkout-php": "^0.4.0",
        "baddiservices/safehtml": "^0.0.6",
        "bugsnag/bugsnag-laravel": "^2.0",
        "creativeorange/gravatar": "^1.0",
        "donatello-za/rake-php-plus": "^1.0",
        "fakerphp/faker": "^1.23",
        "google/apiclient": "^2.16",
        "guzzlehttp/guzzle": "^7.8",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^11.0",
        "laravel/tinker": "^2.9",
        "onecentlin/laravel-adminer": "^7.0",
        "propaganistas/laravel-phone": "^5.0",
        "spatie/laravel-robots-middleware": "^1.3",
        "stevebauman/location": "^7.0",
        "stripe/stripe-php": "^13.0"
    },
    "require-dev": {
        "laravel/sail": "^1.29",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "BADDIServices\\ClnkGO\\": "app/",
            "BADDIServices\\Framework\\": "core/src",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "chmod -R 755 public/"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "google/apiclient-services": [
            "Google My Business"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "platform": {
            "php": "8.4.0"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
